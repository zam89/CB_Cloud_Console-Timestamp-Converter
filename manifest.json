{
  "manifest_version": 3,
  "name": "Carbon Black Cloud Timestamp Converter",
  "version": "1.0",
  "description": "Automatically convert Carbon Black Cloud timestamp to UTC. Author: mohdkhairulazam",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      //"matches": ["<all_urls>"],
      "matches": ["*://defense-prod.conferdeploy.net/*"], // Add your desired URL pattern here
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
